=form_for(@user, url: sign_up_path) do |f| 
	
	=f.label :first_name, "First Name"
	=f.text_field :first_name

	=f.label :last_name, "Last Name"
	=f.text_field :last_name

	=f.label :email, "Email"
	=f.text_field :email

	=f.label :password, "Password"
	=f.password_field :password

	=f.submit 'Register'