=form_for(@user, url: sign_up_path) do |f| 
	
	=f.label :first_name
	=f.text_field :first_name

	=f.label :last_name
	=f.text_field :last_name

	=f.label :password
	=f.password_field :password

	=f.label :height
	=f.text_field :height

	=f.label :weight
	=f.number_field :weight

	=f.submit 'Sign Up'